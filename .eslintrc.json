{
  "extends": [
    "airbnb",
    "eslint:recommended",
    "plugin:react/recommended",
    "prettier"
  ],
  "rules": {
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "react/forbid-prop-types": [false],
    "react/jsx-one-expression-per-line": [false]
  },
  "env": { "browser": true },
  "parser": "babel-eslint"
}
